{
  "name": "vue2-flash",
  "description": "A Vue.js 2 Helpfull Notification.",
  "version": "1.0.3",
  "main": "dist/vue-flash.js",
  "author": "sagaryonjan",
  "private": false,
  "scripts": {
    "dev": "webpack",
    "pro": "set NODE_ENV=production&&webpack",
    "watch": "npm run dev -- --watch",
    "test": "jest",
    "watch:test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sagaryonjan/vue-flash.git"
  },
  "bugs": {
    "url": "https://github.com/sagaryonjan/vue-flash/issues"
  },
  "license": "MIT",
  "keywords": [
    "vue",
    "notification",
    "flash",
    "notify"
  ],
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.10",
    "babel-core": "^6.26.0",
    "babel-install": "2.1.0",
    "babel-jest": "^22.0.6",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "cross-env": "^5.1",
    "css-loader": "^0.28.7",
    "expect": "^22.0.6",
    "extract-text-webpack-plugin": "^3.0.2",
    "jest": "^22.0.6",
    "jest-css-modules": "^1.1.0",
    "jest-serializer-vue": "^0.3.0",
    "jsdom": "^11.5.1",
    "jsdom-global": "^3.0.2",
    "mocha": "^4.1.0",
    "mocha-webpack": "^1.0.1",
    "vue": "^2.5.7",
    "vue-hot-reload-loader": "^0.1.3",
    "vue-jest": "^1.4.0",
    "vue-loader": "^11.3.4",
    "vue-test-utils": "^1.0.0-beta.11",
    "webpack": "^3.8.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "transform": {
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest",
      "^.+\\.js$": "babel-jest",
      "^.+\\.(css|scss|less)$": "jest-css-modules"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,vue}",
      "!**/node_modules/**"
    ],
    "coverageReporters": [
      "html",
      "text-summary"
    ],
    "mapCoverage": true
  },
  "dependencies": {
    "sinon": "^4.1.4",
    "vue-template-compiler": "^2.5.13",
    "webpack-merge": "^4.1.1"
  }
}
